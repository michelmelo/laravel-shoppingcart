{
    "name": "michelmelo/laravel-shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [{
        "name": "Michel Melo",
        "email": "eu@michelmelo.pt"
    }, {
        "name": "Rob Gloudemans",
        "email": "info@robgloudemans.nl"
    }],
    "require": {
        "illuminate/support": "5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.*.* || 7.*.*",
        "illuminate/session": "5.5.* || 5.6.* || 5.7.* || 5.8.* || 5.8.* || 6.*.* || 7.*.*",
        "illuminate/events": "5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.*.* || 7.*.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0 || ~6.0 || ~7.0 || ~7.5 || ~8.0 || ~8.5",
        "mockery/mockery": "~0.9.0 || ~1.0 || ~1.3.1",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichelMelo\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "MichelMelo/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "MichelMelo\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "MichelMelo\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}