{
    "name": "michelmelo/laravel-shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Melo",
            "email": "eu@michelmelo.pt"
        },
        
    ],
    "require": {
        "illuminate/support": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
        "illuminate/session": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
        "illuminate/events": "5.4.*||5.5.*||5.6.*||5.7.*||5.8.*||^6.0||^7.0||^8.0||^9.0",
        "nesbot/carbon": "~1.20||^1.26.3||^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0||~6.0||~7.0||~8.0||~9.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.4|^4.0|^5.0|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichelMelo\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "MichelMelo\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "MichelMelo\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
